{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
  "data": {
    "url": "data/seattle-weather.csv"
  },
  "height": 150,
  "width": 500,
  "transform": [
    {
      "calculate": "datum.temp_max - datum.temp_min",
      "as": "temp_range"
    }
  ],
  "mark": {
    "type": "line",
    "interpolate": "monotone"
  },
  "encoding": {
    "x": {
      "field": "date",
      "timeUnit": "month"
    },
    "y": {
      "field": "temp_range",
      "aggregate": "mean"
    },
    "color": {
      "timeUnit": "year",
      "type": "nominal",
      "field": "date",
      "title": "Year"
    }
  },
  "config": {}
}